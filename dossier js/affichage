let userconnect = JSON.parse(localStorage.getItem("sex1")) || [];
console.log(userconnect);
let nomEdit = document.querySelector(".nom");
let noms = document.querySelector(".noms");
let emailEdit = document.querySelector(".emails");
let boutonEdit = document.querySelector("#btedit");
nomEdit.innerHTML = userconnect.nom;
noms.innerHTML = userconnect.nom;
console.log(userconnect.nom);
emailEdit.innerHTML = userconnect.email;
boutonEdit.href = `./tbedit.html?id=${userconnect.id}`;
